<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.pikefin.businessobjects.SecurityAccounts" table="security_accounts">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="username" type="string">
            <column name="username" length="100" not-null="true" />
        </property>
        <property name="password" type="string">
            <column name="password" length="100" not-null="true" />
        </property>
        <property name="auth1" type="string">
            <column name="auth1" length="50" not-null="true" />
        </property>
        <property name="auth2" type="string">
            <column name="auth2" length="50" not-null="true" />
        </property>
        <property name="auth3" type="string">
            <column name="auth3" length="50" not-null="true" />
        </property>
        <property name="auth4" type="string">
            <column name="auth4" length="50" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
