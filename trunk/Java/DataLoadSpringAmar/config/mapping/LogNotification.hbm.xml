<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.pikefin.businessobjects.LogNotification" table="log_notifications" dynamic-update="true">
		<id name="logNotificationId" column="id" type="integer">
			<generator class="identity"/>
		</id>
		<property name="count" column="count" type="integer" />
		<property name="processBegin" column="process_begin" type="calendar" />
		<property name="processEnd" column="process_end" type="calendar" />
		
 
   </class>
</hibernate-mapping>
