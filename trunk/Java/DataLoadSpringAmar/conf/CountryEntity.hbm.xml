<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pikefin.hibernate.CountryEntity" table="countries_entities" dynamic-update="true">
		<id name="countryEntityId" column="id" type="integer">
			<generator class="identity"/>
		</id>
		<property name="entityId" column="entity_id" type="integer"/>
		<property name="countryId" column="country_id" type="integer"/>
		<property name="defaultCountry" column="entity_id" type="integer"/>

   </class>
</hibernate-mapping>
