<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pikefin.hibernate.Country" table="countries" dynamic-update="true">
		<id name="countryId" column="id" type="integer">
			<generator class="identity"/>
		</id>
		<property name="name" column="name" type="string"/>
		<property name="hash" column="hash" type="string"/>
		<property name="shortName" column="short_name" type="string"/>
   </class>
</hibernate-mapping>
