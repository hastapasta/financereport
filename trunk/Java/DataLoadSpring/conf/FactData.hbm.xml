<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pikefin.hibernate.FactData" table="fact_data" dynamic-update="true">
		<id name="factDataId" column="id" type="integer">
			<generator class="identity"/>
		</id>
		<property name="value" column="value" type="float" />
		<property name="dateCollected" column="date_collected" type="calendar" />
   </class>
</hibernate-mapping>
