<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.pikefin.businessobjects.Country" table="countries" >
		<id name="countryId" column="id" type="integer">
			<generator class="identity"/>
		</id>
		<property name="name" column="name" type="string"/>
		<property name="hash" column="hash" type="string"/>
		<property name="shortName" column="short_name" type="string"/>
		<set name="countryAliasList" table="country_aliases">
		    <key column="country_id"></key>
		    <one-to-many class="com.pikefin.businessobjects.CountryAlias"/>
		</set>
   </class>
</hibernate-mapping>
